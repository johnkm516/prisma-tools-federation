datasource db {
  provider = "postgresql"
  url      = "DATABASE_URL"
}

generator client {
  provider        = "prisma-client-js"
}


model User {
  id            Int      @id @default(autoincrement())
  createdAt     DateTime @default(now())
  username      String?  @unique
  password      String?
  email         String   @unique
  roles         String[] 
  googleId      String?  @unique
  googleProfile Json?
}
